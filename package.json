    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "adm-zip": "0.5.9",
    "bootstrap": "^5.1.3",
    "cross-env": "^7.0.3",
    "electron": "13.6.2",
    "electron-builder": "^22.14.5",
    "electron-reload": "^1.5.0",
    "eslint": "^8.13.0",
