    "@angular/material": "15.2.7",
    "@angular/platform-browser": "^15.2.7",
    "@angular/platform-browser-dynamic": "15.2.7",
    "@angular/router": "15.2.7",
    "@ngneat/transloco": "4.2.6",
    "@ngneat/transloco-locale": "4.0.1",
    "date-fns": "2.30.0",
    "exceljs": "4.3.0",
    "filenamify": "6.0.0",
    "html-to-image": "1.11.11",
    "jspdf": "2.5.1",
    "jspdf-autotable": "3.5.28",
    "nanoid": "4.0.2",
    "ngx-editor": "15.3.0",
    "rxjs": "7.8.1",
    "tslib": "2.5.0",
    "zone.js": "0.13.0"
  },
