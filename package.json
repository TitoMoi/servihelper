{
  "name": "servihelper",
  "version": "4.3.0",
  "description": "Servihelper",
  "author": {
    "name": "MoisÃ©s Medina Vicente",
    "email": "servihelper@gmail.com"
  },
  "keywords": [
    "servihelper"
  ],
  "main": "app/main.js",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "tsc": "tsc",
    "tsc:main": "tsc ./app/main.ts",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "ng build -c production",
    "build:dev": "npm run build -- -c dev",
    "ng:serve": "ng serve -c dev",
    "electron:serve-tsc": "tsc -p tsconfig.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build && npx electron .",
    "electron:build": "electron-builder build --publish=never",
    "prepare:publish:windows": "npm run build && node scripts/clean-release.js && npm run electron:build && node scripts/zip-release-windows.js",
    "prepare:publish:mac:universal": "npm run build && node scripts/clean-release.js && npm run electron:build -- --universal",
    "prepare:publish:linux": "npm run build && node scripts/clean-release.js && npm run electron:build",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "lint": "ng lint",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find"
  },
  "dependencies": {
    "@angular/animations": "^15.2.7",
    "@angular/cdk": "^15.2.7",
    "@angular/common": "^15.2.7",
    "@angular/compiler": "^15.2.7",
