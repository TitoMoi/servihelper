{
  "name": "servihelper",
  "version": "5.7.3",
  "description": "Servihelper",
  "author": {
    "name": "MoisÃ©s Medina Vicente",
    "email": "servihelper@gmail.com"
  },
  "keywords": [
    "servihelper"
  ],
  "main": "app/main.js",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "tsc": "tsc",
    "tsc:main": "tsc ./app/main.ts",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "ng build -c production",
    "build:debug": "ng build -c debug",
    "ng:serve": "ng serve -c dev",
    "transloco:optimize": "transloco-optimize dist/assets/i18n",
    "electron:serve-tsc": "tsc -p tsconfig.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build && npx electron .",
    "electron:build": "electron-builder build --publish=never",
    "release:windows": "npm run electron:build && npm run clean:release",
    "release:mac:universal": "npm run electron:build -- --universal && npm run clean:release",
    "release:linux": "npm run electron:build && npm run clean:release",
    "clean:release": "node scripts/clean-release.js",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "lint": "ng lint",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find",
    "i18n:find:add": "transloco-keys-manager find --add-missing-keys && node scripts/replace-i18json.js"
  },
  "dependencies": {
    "@angular/animations": "~17.1.0",
    "@angular/cdk": "~17.1.0",
    "@angular/common": "~17.1.0",
    "@angular/compiler": "~17.1.0",
    "@angular/core": "~17.1.0",
    "@angular/forms": "~17.1.0",
    "@angular/language-service": "~17.1.0",
    "@angular/material": "~17.1.0",
    "@angular/platform-browser": "~17.1.0",
    "@angular/platform-browser-dynamic": "~17.1.0",
    "@angular/router": "~17.1.0",
    "@ngneat/transloco": "6.0.4",
    "@ngneat/transloco-locale": "5.1.1",
    "@ngneat/transloco-optimize": "^5.0.3",
    "@types/leaflet": "^1.9.8",
    "date-fns": "3.0.6",
    "filenamify": "6.0.0",
    "html-to-image": "1.11.11",
    "jspdf": "2.5.1",
    "jspdf-autotable": "3.7.0",
    "leaflet": "^1.9.4",
    "nanoid": "^5.0.1",
    "ngx-editor": "^16.0.1",
    "pako": "^2.1.0",
    "rxjs": "7.8.1",
    "tslib": "^2.6.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "~17.0.0",
    "@angular-devkit/build-angular": "~17.1.0",
    "@angular-eslint/builder": "17.2.1",
    "@angular-eslint/eslint-plugin": "17.2.1",
    "@angular-eslint/eslint-plugin-template": "17.2.1",
    "@angular-eslint/schematics": "17.2.1",
    "@angular-eslint/template-parser": "17.2.1",
    "@angular/cli": "~17.1.0",
    "@angular/compiler-cli": "~17.1.0",
    "@electron/universal": "2.0.1",
    "@ngneat/transloco-keys-manager": "3.7.0",
    "@types/fs-extra": "~11.0.0",
    "@types/node": "18.16.3",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "adm-zip": "0.5.10",
    "bootstrap": "~5.3.0",
    "cross-env": "7.0.3",
    "electron": "28.0.0",
    "electron-builder": "24.9.1",
    "electron-reload": "2.0.0-alpha.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "~8.8.0",
    "eslint-plugin-import": "~2.27.5",
    "eslint-plugin-jsdoc": "~44.2.5",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "^5.1.3",
    "fs-extra": "~11.0.0",
    "nan": "2.17.0",
    "normalize.css": "8.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "3.2.4",
    "sort-json": "2.0.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "wait-on": "~7.0.1",
    "webdriver-manager": "12.1.9"
  },
  "optionalDependencies": {
    "dmg-license": "1.0.11"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "chrome 91",
    "last 2 Safari versions"
  ]
}
